[data-sizing-toggle] {
    display: none
}

[data-brands-toggle] {
    display: none
} 

[data-shipping-toggle] {
    display: none
}

.Product-content-3 {
    display: none;
}

.Product-content-2 {
    display: none;
}

.Product-content-1 {
    display: none;
}

.sky-pilot {
    text-align: center;
    margin-top: 40px;
}

.CategoryBanner {
    text-align: center;
    
}
.collection-colour{
    background: rgba(0, 0, 0, 0.07);
}

.CategoryBanner div h2 {
    font-family: {{settings.type_base_font_family.family}}!important;
    margin-top:10px !important;
    font-weight:normal;
    font-size:17px;
    width:820px;
    padding: 10px 0px 20px;
    margin: 5px 10px;

    display: inline-block;
    
    vertical-align: middle;
    
    line-height: 1.2;
}

.Ethicalness,
.ethical-icons,
.img-ethical {
    text-align: center
}

.Ethicalness {
    display: block
}

.img-ethical {
    display: inline-block;
    width: 85px
}

.goog-tooltip {
    display: none !important;
}

.goog-tooltip:hover {
    display: none !important;
}

.goog-text-highlight {
    background-color: transparent !important;
    border: none !important;
    box-shadow: none !important;
}

.CategoryBanner div h1 {
    display: none
}

.rte h2 {
    margin-bottom: 0;
}

.FoldorOverGridhover:hover {
    opacity: .85
}

.FoldorOverGrid {
    display: inline-block
}

.FoldorOverGrid img {
    width: 100%
}

.FoldorOverGrid .FoldorOverGrid2x {
    margin: 0 .5% .5%;
    display: block;
    width: 49%;
    float: left
}

.FoldorOverGrid .FoldorOverGrid3x {
    margin: 0 .5% .5%;
    display: block;
    width: 32%;
    float: left
}

.FoldorOverGrid .FoldorOverGrid4x {
    margin: 0 .5% .5%;
    display: block;
    width: 24%;
    float: left
}

.FoldorOverGridBorer {
    font-size: 16px
}

.hidden-lg {
    display: block;
}

.hidden-xs {
    display: none;
}

@media(max-width:768px) {
    .hidden-lg {
        display: none !important;
    }

    .hidden-xs {
        display: block !important;
    }

    .FoldorOverGrid .FoldorOverGrid5x {
        margin: 0 .5% .2% !important;
        width: 97% !important;
    }

    .FoldorOverGrid .FoldorOverGrid5x img {
        margin: 20px .5% .2% !important;
        width: 97% !important;
        max-width: 250px;
    }

    .FoldorOverGrid .FoldorOverGrid2x {
        margin: 0 .5% .2%;
        width: 97%
    }

    .FoldorOverGridBorer a {
        position: relative;
        color: #444
    }

    .FoldorOverGridBorer {
        border: 1px solid#444;
        padding: 8px 10px;
        margin: 0 -1px -1px 0 !important;
        display: block !important;
        width: 50% !important;
        text-align: center
    }

    .FoldorOverGrid .FoldorOverGrid3x,
    .FoldorOverGrid .FoldorOverGrid4x {
        margin: 0 .5% .2%;
        width: 97%
    }
}

@media(max-width:1000px)and(min-width:769px) {
    .FoldorOverGridBorer a {
        position: relative;
        color: #444
    }

    .FoldorOverGridBorer {
        border: 1px solid#444;
        padding: 8px 10px;
        margin: 0 -1px -1px 0 !important;
        display: block !important;
        width: 50% !important;
        text-align: center
    }

    .FoldorOverGrid .FoldorOverGrid4x {
        margin: 0 8px 16px;
        width: 47%
    }
}

@media(max-width:480px) {
    .FoldorOverGridBorer {
        padding: 8px 5px !important;
        font-size: 11px
    }
}

.YogiGlobal{
    font-family: {{settings.type_header_font_family.family}} !important;
}
.ssw-dropdown-menu{
    top: 80%!important;
right: initial!important;
}